{
  "name": "history",
  "version": "3.3.0",
  "scripts": {
    "harness": "node ./node_modules/tuxharness/index.js",
    "lint": "npm run lint:ui && npm run lint:js && npm run lint:docs",
    "lint:js": "eslint app server *.js --ext .js,.jsx",
    "lint:ui": "eslint ui --ext .jsx",
    "lint:docs": "eslint --ext md --config .eslintrc-markdown .",
    "lint:filter": "eslint --ext .js,.jsx",
    "release": "standard-version",
    "start": "npm run webpack && node .",
    "test": "npm run webpack && nyc --extension .jsx babel-tape-runner server/plugins/*/test/*.spec.js",
    "test:cover": "nyc report --reporter=html",
    "test:react": "babel-tape-runner server/plugins/*/test/*.enzyme.jsx app/components/*/test/*.enzyme.jsx | faucet",
    "test:jest": "jest ui",
    "debug:test:react": "node $NODE_DEBUG_OPTION ./node_modules/babel-tape-runner/bin/babel-tape-runner server/plugins/*/test/*.enzyme.jsx | faucet",
    "webpack": "webpack --config webpack.js",
    "ci-cover:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "ci-cover:prep-codeclimate": "nyc report --reporter=text-lcov > coverage.lcov",
    "ci-cover:codeclimate": "codeclimate-test-reporter < coverage.lcov"
  },
  "main": "server/index.js",
  "dependencies": {
    "ajv": "^6.5.3",
    "app-root-path": "^2.0.1",
    "async": "^2.1.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.16.0",
    "boom": "^7.0.0",
    "camelcase": "^5.0.0",
    "clone": "^2.1.1",
    "dot-prop": "^4.0.0",
    "dotenv": "^6.0.0",
    "dropbox": "^4.0.0",
    "extend": "^3.0.0",
    "geojson": "^0.5.0",
    "glob": "^7.0.0",
    "gm": "^1.23.0",
    "hapi": "^17.2.0",
    "hapi-react-views": "^10.0.0",
    "hapi-swagger": "^9.1.1",
    "inert": "^5.1.0",
    "joi": "^13.0.0",
    "jquery-colorbox": "^1.6.4",
    "lout": "^11.0.0",
    "mapbox-gl": "^0.48.0",
    "mime-types": "^2.1.12",
    "mkdirp": "^0.5.0",
    "moment": "^2.20.1",
    "node-notifier": "^5.0.1",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dates": "^17.1.1",
    "react-dom": "^16.0.0",
    "react-mapbox-gl": "^3.8.0",
    "react-moment-proptypes": "^1.5.0",
    "react-test-renderer": "^16.0.0",
    "redux-saga": "^0.16.0",
    "rimraf": "^2.2.8",
    "styled-components": "^3.1.6",
    "tap-webpack-plugin": "^2.0.0",
    "tuxharness": "^2.0.4",
    "typy": "^2.0.0",
    "vision": "^5.3.1",
    "webpack": "^4.0.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "@mapbox/geojsonhint": "^2.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "calipers": "^2.0.0",
    "calipers-jpeg": "^2.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-redux-saga": "^0.9.0",
    "expect.js": "^0.3.0",
    "faucet": "^0.0.1",
    "file-loader": "^1.0.0",
    "jest": "^23.4.2",
    "jsdom": "^12.0.0",
    "lodash": "^4.17.10",
    "node-fetch": "^2.0.0",
    "nyc": "^12.0.2",
    "sinon": "^6.0.1",
    "standard-version": "^4.0.0",
    "style-loader": "^0.22.0",
    "tape": "^4.6.3",
    "tape-catch": "^1.0.6",
    "url-loader": "^1.0.1",
    "webpack-cli": "^3.1.0",
    "wreck": "^14.0.2"
  },
  "description": "Your personal history storyboarded with photo and video albums. Associate photos with their meta data including geocode, caption, friends (characters)... in XML albums.",
  "repository": {
    "type": "git",
    "url": "https://github.com/danactive/history.git"
  },
  "keywords": [
    "photo",
    "gallery",
    "geocode",
    "slippy",
    "map",
    "video",
    "showcase"
  ],
  "author": {
    "name": "danactive",
    "url": "http://twitter.com/danactive"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danactive/history/issues"
  },
  "homepage": "https://github.com/danactive/history",
  "engines": {
    "node": ">=8.6.0",
    "npm": ">=5.3.0"
  },
  "readmeFilename": "README.md",
  "tuxharness": "./tuxharness.js",
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(jest).js?(x)"
    ]
  },
  "nyc": {
    "exclude": [
      "**/cases.js",
      "**/*.spec.js"
    ]
  }
}
