{
  "name": "history",
  "version": "7.1.0",
  "scripts": {
    "harness": "node ./node_modules/tuxharness/index.js",
    "lint": "npm run lint:js && npm run lint:docs",
    "lint:js": "eslint server *.js --ext .js,.jsx",
    "lint:docs": "eslint --ext md --config .eslintrc-markdown.js .",
    "lint:filter": "eslint --ext .js,.jsx",
    "release": "standard-version",
    "start": "npm run webpack && node .",
    "test": "npm run test:tape && npm run webpack && nyc --extension .jsx",
    "test:tape": "nyc --extension .jsx tape server/plugins/*/test/*.spec.js",
    "test:cover": "nyc report --reporter=html",
    "test:jest": "jest",
    "webpack": "webpack --config webpack.js",
    "ci-cover:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "ci-cover:prep-codeclimate": "nyc report --reporter=text-lcov > coverage.lcov",
    "ci-cover:codeclimate": "codeclimate-test-reporter < coverage.lcov"
  },
  "main": "server/index.js",
  "dependencies": {
    "@hapi/hapi": "^21.4.0",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.3",
    "app-root-path": "^3.1.0",
    "async": "^2.6.4",
    "boom": "^7.0.0",
    "camelcase": "^6.3.0",
    "clone": "^2.1.1",
    "dot-prop": "^5.3.0",
    "dotenv": "^8.6.0",
    "dropbox": "^4.0.30",
    "extend": "^3.0.0",
    "file-loader": "^6.2.0",
    "geojson": "^0.5.0",
    "glob": "^7.2.3",
    "gm": "^1.25.0",
    "hapi-react-views": "^11.0.0",
    "hapi-swagger": "^15.0.0",
    "isomorphic-fetch": "^3.0.0",
    "joi": "^17.9.2",
    "jquery-colorbox": "^1.6.4",
    "mime-types": "^2.1.35",
    "node-notifier": "^9.0.1",
    "process": "^0.11.10",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "sharp": "^0.32.6",
    "styled-components": "^4.4.1",
    "url-loader": "^4.1.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/register": "^7.27.1",
    "@hapi/wreck": "^17.2.0",
    "@mapbox/geojsonhint": "^3.2.0",
    "babel-loader": "^10.0.0",
    "calipers": "^2.0.0",
    "calipers-jpeg": "^2.0.0",
    "cheerio": "^0.22.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.1.1",
    "css-loader": "^6.11.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^29.7.0",
    "jsdom": "^15.2.1",
    "lodash": "^4.17.21",
    "nyc": "^14.1.1",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.4",
